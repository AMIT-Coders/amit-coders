{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/components/HeroHomepage/HeroHomepage.jsx", "app/components/FAQsHomepage/FAQsHomepage.tsx", "app/components/Navbar/Navbar.jsx", "app/components/Footer/Footer.tsx", "app/components/Dropdown/Dropdown.js"],
  "exclude": ["node_modules"]
}
